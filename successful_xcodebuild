#!/bin/bash

# xcodebuild task built based on the one generated by gradle-xcodePlugin, but 
# with the addition of the -project option.
xcodebuild \
  -project ./GradleXcodeIssueDemo/GradleXcodeIssueDemo.xcodeproj \
  -scheme GradleXcodeIssueDemo \
	-configuration Release \
  -destination 'platform=iOS Simulator,OS=9.3,name=iPhone 6'
